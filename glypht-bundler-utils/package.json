{
  "name": "@glypht/bundler-utils",
  "version": "0.0.7",
  "description": "Higher-level functionality for bundling, splitting, and transforming font files",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./unicode-ranges.js": {
      "import": {
        "types": "./dist/unicode-ranges.d.ts",
        "default": "./dist/unicode-ranges.js"
      }
    },
    "./feature-metadata.js": {
      "import": {
        "types": "./dist/feature-metadata.d.ts",
        "default": "./dist/feature-metadata.js"
      }
    }
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "prepack": "rollup -c rollup.config.js",
    "watch": "rollup --watch -c rollup.config.js"
  },
  "files": [
    "dist/**/*.{js,ts,wasm}",
    "README.md"
  ],
  "author": "valadaptive",
  "license": "(MIT OR ISC OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valadaptive/glypht.git",
    "directory": "glypht-bundler-utils"
  },
  "homepage": "https://glypht.valadaptive.dev/docs/api/modules/_glypht_bundler-utils.html",
  "dependencies": {
    "@glypht/core": "^0.0.7"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "rollup": "^4.44.2"
  }
}
